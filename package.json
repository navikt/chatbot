{
    "name": "@navikt/nav-chatbot",
    "version": "1.3.7",
    "description": "Chatbot Frida for nav.no",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/chatbot.git"
    },
    "main": "umd/nav-chatbot.min.js",
    "types": "umd/index.d.ts",
    "files": [
        "umd"
    ],
    "scripts": {
        "publish:beta": "npm run build && npm version prerelease --preid beta && npm publish --access public --tag beta",
        "build": "npm run generate:types && nwb build-react-component --no-demo && import-path --path umd/nav-chatbot.min.js",
        "clean": "nwb clean-module && nwb clean-demo",
        "start": "nwb -c ./nwb.demo.config.js serve-react-demo --copy-files",
        "start:host": "nwb -c ./nwb.demo.config.js serve-react-demo --copy-files --host 0.0.0.0",
        "generate:types": "dts-generator --out src/index.d.ts --project ./ --exclude ./src/components/**/styles.ts --exclude ./node_modules/**/*.d.ts --prefix",
        "test": "xo && nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "classnames": "^2.2.6",
        "js-cookie": "^2.2.1",
        "lodash.throttle": "^4.1.1",
        "md5": "^2.3.0",
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "@navikt/fnrvalidator": "^1.1.3",
        "moment": "^2.27.0",
        "nav-frontend-alertstriper": "^3.0.23",
        "nav-frontend-alertstriper-style": "^2.0.17",
        "nav-frontend-core": "^5.0.1",
        "nav-frontend-ikoner-assets": "^2.0.3",
        "nav-frontend-js-utils": "^1.0.9",
        "nav-frontend-knapper": "^2.0.9",
        "nav-frontend-knapper-style": "^1.0.1",
        "nav-frontend-lenker": "^1.0.41",
        "nav-frontend-lenker-style": "^0.2.29",
        "nav-frontend-paneler-style": "^0.3.21",
        "nav-frontend-skjema": "^3.0.18",
        "nav-frontend-skjema-style": "^2.0.10",
        "nav-frontend-typografi": "^2.0.26",
        "nav-frontend-typografi-style": "^1.0.22",
        "react": "16.13.1",
        "react-app-polyfill": "1.0.6",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@navikt/fnrvalidator": "^1.1.3",
        "@types/js-cookie": "^2.2.6",
        "@types/md5": "^2.2.0",
        "@types/react": "^16.8.24",
        "@types/react-dom": "^16.8.5",
        "@types/styled-components": "^5.1.2",
        "dts-generator": "^3.0.0",
        "import-path": "^1.0.14",
        "less": "^3.12.2",
        "less-loader": "^7.0.1",
        "moment": "^2.27.0",
        "moment-locales-webpack-plugin": "^1.2.0",
        "nav-frontend-alertstriper": "^3.0.23",
        "nav-frontend-alertstriper-style": "^2.0.17",
        "nav-frontend-core": "^5.0.1",
        "nav-frontend-ikoner-assets": "^2.0.3",
        "nav-frontend-js-utils": "^1.0.9",
        "nav-frontend-knapper": "^2.0.9",
        "nav-frontend-knapper-style": "^1.0.1",
        "nav-frontend-lenker": "^1.0.41",
        "nav-frontend-lenker-style": "^0.2.29",
        "nav-frontend-paneler-style": "^0.3.21",
        "nav-frontend-skjema": "^3.0.18",
        "nav-frontend-skjema-style": "^2.0.10",
        "nav-frontend-typografi": "^2.0.26",
        "nav-frontend-typografi-style": "^1.0.22",
        "nwb": "^0.25.2",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.8.6",
        "styled-components": "^5.1.1",
        "svg-inline-loader": "^0.8.0",
        "ts-loader": "^8.0.2",
        "typescript": "^3.9.7",
        "xo": "^0.34.1"
    },
    "prettier": {
        "singleQuote": true,
        "jsxSingleQuote": true,
        "tabWidth": 4,
        "bracketSpacing": false
    },
    "xo": {
        "envs": ["browser"],
        "space": true,
        "prettier": true,
        "rules": {
            "@typescript-eslint/no-implicit-any-catch": 0,
            "import/no-unassigned-import": [
                "error",
                {
                    "allow": [
                        "@babel/polyfill",
                        "react-app-polyfill/**",
                        "**/register",
                        "**/register/**"
                    ]
                }
            ]
        },
        "overrides": [
            {
                "files": "tests/**/*",
                "envs": ["phantomjs", "mocha", "browser"]
            }
        ]
    }
}

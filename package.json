{
    "name": "@navikt/nav-chatbot",
    "version": "1.0.0",
    "description": "Chatbot Frida for nav.no",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/chatbot.git"
    },
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "build": "nwb build-react-component --no-demo",
        "build:test-app": "nwb -c ./nwb.demo.config.js build-react-app --no-demo",
        "clean": "nwb clean-module && nwb clean-demo",
        "prepublishOnly": "npm run build",
        "start": "nwb -c ./nwb.demo.config.js serve-react-demo --copy-files",
        "test": "nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "moment": "^2.24.0",
        "react-moment": "^0.9.2",
        "styled-components": "^4.3.2"
    },
    "peerDependencies": {
        "react": "16.x"
    },
    "devDependencies": {
        "@types/react": "^16.8.24",
        "@types/react-dom": "^16.8.5",
        "@types/styled-components": "^4.1.18",
        "nwb": "^0.23.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "svg-inline-loader": "^0.8.0",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3"
    },
    "prettier": {
        "singleQuote": true,
        "jsxSingleQuote": true,
        "tabWidth": 4
    }
}
